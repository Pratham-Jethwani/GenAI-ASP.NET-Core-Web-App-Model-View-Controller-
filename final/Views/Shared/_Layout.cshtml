<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - final</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/MyStyle.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-white bg-rgba(1,1,1,0.4)">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">AI</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="jobs">Jobs</a>
                        </li>


                        <li class="nav-item">
                            <a class="nav-link text-white" href="#1" id="genai-link">GenAI sites</a>
                        </li>


                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
                        </li>

                        


                        

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
   
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-white bg-rgba(1,1,1,0.4)">
            <div class="container-fluid">
                
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index" style="margin-right:25vw">Home</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="jobs" style="margin-right:25vw">Jobs</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="contact" style="margin-right:25vw">About Us</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="" style="margin-right:3vw">Follow Us</a>
                        </li>
                    </ul>
                    
                </div>
            </div>
        </nav>
    </footer>

    <footer>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-white bg-rgba(1,1,1,0.4)">
            <div class="container-fluid">

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">

                        <li class="nav-item">
                            <a class="nav-link text-white" href="Home/#1" id="genai-link" style="margin-right:23vw">GenAI sites</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="contact" style="margin-right:25vw">Contact</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="contact" style="margin-right:20vw">Copyright Info</a>
                        </li>

                        <li>
                            <a href="https://plus.google.com/" target="_blank" rel="noopener noreferrer">
                                <img src="~/images/icon/google-icon.png" alt="Google" class="social-icon">
                            </a>

                            <!-- YouTube icon -->
                            <a href="https://www.youtube.com/" target="_blank" rel="noopener noreferrer">
                                <img src="~/images/icon/youtube-icon.png" alt="YouTube" class="social-icon">
                            </a>

                            <!-- Twitter icon -->
                            <a href="https://twitter.com/" target="_blank" rel="noopener noreferrer">
                                <img src="~/images/icon/twitter-icon.png" alt="Twitter" class="social-icon">
                            </a>

                            <!-- Facebook icon -->
                            <a href="https://www.facebook.com/" target="_blank" rel="noopener noreferrer">
                                <img src="~/images/icon/facebook-icon.png" alt="Facebook" class="social-icon">
                            </a>

                            <!-- LinkedIn icon -->
                            <a href="https://www.linkedin.com/" target="_blank" rel="noopener noreferrer">
                                <img src="~/images/icon/linkedin-icon.png" alt="LinkedIn" class="social-icon">
                            </a>
                        </li>


                    </ul>

                </div>
            </div>
        </nav>
    </footer>
    
    
    <script>
        $(document).ready(function () {
            $("#genai-link").on("click", function (event) {
                event.preventDefault(); // Prevent the default behavior of the link

                // Scroll to the element with id "1" on the home page
                $("html, body").animate({
                    scrollTop: $("#1").offset().top
                }, 1000); // Adjust the duration as needed
            });
        });
    </script>


  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
